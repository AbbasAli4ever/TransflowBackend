{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "dotenv -e .env.development -- nest start --watch",
    "start:prod": "node dist/main.js",
    "prisma:generate": "dotenv -e .env.development -- prisma generate",
    "prisma:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:seed": "dotenv -e .env.development -- ts-node prisma/seed.ts",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest test/unit",
    "test:integration": "NODE_ENV=development jest test/integration",
    "test:isolation": "NODE_ENV=development jest test/integration/tenant-isolation",
    "test:all": "npm run test:unit && npm run test:integration",
    "generate:api-types": "openapi-typescript http://localhost:3000/api/docs-json -o ../Documentation/api-types.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/swagger": "^11.2.6",
    "@prisma/client": "^6.19.2",
    "@types/multer": "^2.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "csv-parse": "^6.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.19.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/testing": "^11.1.12",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "jest": "^30.2.0",
    "openapi-typescript": "^7.13.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
